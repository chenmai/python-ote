import mysql.connector;

try:
    conn = mysql.connector.connect(host='localhost', port='3306', user='root', password="123456", database="test", use_unicode=True);
    cursor = conn.cursor();
    cursor.execute('select * from web where ID =1');
    # 取回的是列表，列表中包含元组
    list1 = cursor.fetchall();
    print (list1);

    for record in list1:
        print ("Record %d is %s!" % (record[0], record[1]));
        
    cursor.execute('INSERT  web VALUES("2","www.baidu.com","测试更新")')
    

except mysql.connector.Error as e:
    print ('Error : {}'.format(e));
finally:
    cursor.close;
    conn.close;
    print ('Connection closed in finally');
